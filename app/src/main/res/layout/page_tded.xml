<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:orientation="vertical"
  android:background="@drawable/bg">
  
  		<RelativeLayout 
	        android:id="@+id/tded_layout_menu"
	        android:layout_width="fill_parent"
   			android:layout_height="wrap_content" 
   			android:orientation="horizontal" >
   			
   			
			    <ImageView
		  		android:id="@+id/tded_image_menu"
		  		android:layout_width="wrap_content"
		  		android:layout_height="wrap_content"
		  		android:background="@drawable/btn_menu"
		  		android:layout_alignParentLeft="true"
		  		android:layout_gravity="top"
		  		/>
			    
			    <ImageView
		  		android:id="@+id/tded_image_page"
		  		android:layout_width="fill_parent"
		  		android:layout_height="wrap_content"
		  		android:background="@drawable/header_tded"
		  		android:layout_toRightOf="@+id/tded_image_menu"
		  		android:layout_gravity="top"
		  		/>
			    
	    </RelativeLayout> 
    
  <RelativeLayout
  	android:id="@+id/tded_layout_header"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_centerVertical="true"
  	android:gravity="left"
  	android:layout_below="@+id/tded_layout_menu"
  >
  	<ImageView
  		android:id="@+id/tded_header_img"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_alignParentLeft="true"
  		android:src="@drawable/hd_sub_ball"
  	/>
  	
  	<TextView
  		android:id="@+id/tded_header_text"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_toRightOf="@+id/tded_header_img"
  		style="@style/TextBack"
  	/>
  	
  </RelativeLayout>
    <RelativeLayout
  	android:id="@+id/tded_layout_wording"
  	android:layout_width="fill_parent"
  	android:layout_height="wrap_content"
  	android:gravity="center_vertical"
  	android:layout_below="@+id/tded_layout_header"
  >
  	<TextView
  		android:id="@+id/tded_header_wording1"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		style="@style/TextBackBold"
  		android:layout_marginLeft="10dp"
  	/>
  	<TextView
  		android:id="@+id/tded_header_wording2"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_below="@+id/tded_header_wording1"
  		style="@style/TextBackBold"
  		android:layout_marginLeft="10dp"
  		android:layout_marginTop="5dp"
  	/>
  	</RelativeLayout>
  	
  <TextView
  		android:id="@+id/tded_message"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_below="@+id/tded_layout_wording"
  		style="@style/CustomText"
  	/>
  <ExpandableListView
        android:id="@+id/tded_list_data"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:groupIndicator="@drawable/play_expand_background"
        android:listSelector="@drawable/play_expand_seletor"
        android:layout_below="@+id/tded_layout_wording"
        android:scrollbars="none" >
    </ExpandableListView>
</RelativeLayout>
